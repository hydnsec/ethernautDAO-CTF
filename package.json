{
  "name": "ethernautdao-ctf",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "hydn",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "compile": "hardhat compile",
    "node": "hardhat node --network hardhat --show-stack-traces",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:js": "eslint \"**/*.{js,ts}\"",
    "lint:sol": "solhint contracts/**/*.sol",
    "lint:fix": "npm run lint:js:fix && npm run lint:sol:fix",
    "lint:js:fix": "eslint --fix \"**/*.{js,ts}\"",
    "lint:sol:fix": "solhint --fix contracts/**/*.sol",
    "prettier": "npm run prettier:js && npm run prettier:sol",
    "prettier:write": "npm run prettier:js:write && npm run prettier:sol:write",
    "prettier:js": "prettier --check .",
    "prettier:js:write": "prettier --write .",
    "prettier:sol": "prettier --check 'contracts/**/*.sol'",
    "prettier:sol:write": "prettier --write 'contracts/**/*.sol'",
    "deploy:localhost": "hardhat --network localhost deploy",
    "deploy:goerli": "hardhat --network goerli deploy",
    "task:ethernautCarV1:goerli": "hardhat ethernautCarV1 --network goerli --show-stack-traces",
    "task:ethernautCarV2:goerli": "hardhat ethernautCarV2 --network goerli --show-stack-traces",
    "task:ethernautCarV1": "hardhat ethernautCarV1 --network localhost --show-stack-traces",
    "task:ethernautCarV2": "hardhat ethernautCarV2 --network localhost --show-stack-traces"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.1.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "4.7.1",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.9",
    "hardhat": "^2.10.1",
    "hardhat-deploy": "^0.11.12",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-tracer": "^1.1.0-rc.6",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-solidity": "^1.0.0-dev.23",
    "sol2uml": "^2.0.3",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  }
}
